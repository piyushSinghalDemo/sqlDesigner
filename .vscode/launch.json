    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Procees",
            "url": "http://localhost/flowchartdesigner",
            "processId": "${command:PickProcess}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "sourceMaps": true,
            "url": "http://localhost:8082",
            "webRoot": "${workspaceRoot}"
        }
    ]
}